{
    "name": "colibri/framework",
    "description": "Colibri -  PHP micro framework",
    "keywords": [
        "framework",
        "fast",
        "light",
        "colibri",
        "micro"
    ],
    "type": "library",
    "homepage": "http://phpcolibri.com/",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander Chibrikin",
            "email": "alek13.me@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "nesbot/carbon": "^1.22|^2.0",
        "psr/simple-cache": "^1.0",
        "psr/log": "^1.0",
        "swiftmailer/swiftmailer": "^6.0",
        "symfony/console": "^3.4.17 || ^4.1.6 || ^5.0",
        "monolog/monolog": "^1.23"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "friendsofphp/php-cs-fixer": "~2.4",
        "mockery/mockery": "^1.0",
        "filp/whoops": "^2.1"
    },
    "replace": {
        "colibri-fw/application": "self.version",
        "colibri-fw/base": "self.version",
        "colibri-fw/cache": "self.version",
        "colibri-fw/config": "self.version",
        "colibri-fw/console": "self.version",
        "colibri-fw/controller": "self.version",
        "colibri-fw/database": "self.version",
        "colibri-fw/log": "self.version",
        "colibri-fw/mail": "self.version",
        "colibri-fw/migration": "self.version",
        "colibri-fw/pattern": "self.version",
        "colibri-fw/routine": "self.version",
        "colibri-fw/session": "self.version",
        "colibri-fw/util": "self.version",
        "colibri-fw/validation": "self.version",
        "colibri-fw/view": "self.version"
    },
    "autoload": {
        "psr-4": {
            "Colibri\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
